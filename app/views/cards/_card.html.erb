      <li>
      <div class="deck">
        <div class="btn-group">
          <div class="btn disabled" style="width:20px; cursor:pointer;"><i class="icon-move"></i></div>
          <%= link_to [card.deck,card], 'data-content' => "Answer: #{card.back}<br />Updated #{time_ago_in_words card.updated_at} ago", class: "btn pop_me", title: "#{card.front}", style: "width:167px;" do %> <i class="icon-th-list" style="float:left;"></i><%= card.front %> <% end %>
